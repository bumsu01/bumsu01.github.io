    "@vanilla-extract/next-plugin": "^2.3.7",
    "@vanilla-extract/recipes": "^0.5.2",
    "eslint-config-prettier": "^9.1.0",
    "framer-motion": "^11.0.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "next": "14.1.0",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
