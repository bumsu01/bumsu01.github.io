{
  "name": "bumlog",
  "packageManager": "pnpm@8.9.0",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bumsu01.github.io/",
  "scripts": {
    "dev": "rm -rf ./.next/cache && next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "serve": "npx http-server out",
