    "eslint": "^8.57.0",
    "eslint-config-global": "workspace:*",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.35",
